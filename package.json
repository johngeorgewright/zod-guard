{
  "name": "zod-guard",
  "version": "1.1.0",
  "description": "Guard functions for Zod types",
  "main": "dist/zod-guard.js",
  "scripts": {
    "build": "yarn clean && tsc",
    "clean": "rimraf dist",
    "start": "concurrently --names=build,test --prefix-colors=yellow,blue 'yarn start:build' 'yarn start:test'",
    "start:build": "tsc --watch --preserveWatchOutput",
    "start:test": "jest --watch",
    "test": "jest",
    "prepare": "husky install"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/johngeorgewright/zod-guard.git"
  },
  "keywords": [
    "zod",
    "guard"
  ],
  "author": "John Wright <johngeorge.wright@gmail.com>",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/johngeorgewright/zod-guard/issues"
  },
  "homepage": "https://github.com/johngeorgewright/zod-guard#readme",
  "devDependencies": {
    "@commitlint/cli": "17.3.0",
    "@commitlint/config-conventional": "17.3.0",
    "@commitlint/types": "17.0.0",
    "@jest/types": "29.3.1",
    "@semantic-release/commit-analyzer": "9.0.2",
    "@semantic-release/exec": "6.0.3",
    "@semantic-release/git": "10.0.1",
    "@semantic-release/github": "8.0.6",
    "@semantic-release/release-notes-generator": "10.0.3",
    "@types/jest": "29.2.3",
    "@types/node": "18.11.9",
    "@types/source-map-support": "0.5.6",
    "commitizen": "4.2.5",
    "concurrently": "7.6.0",
    "cz-conventional-changelog": "3.3.0",
    "express-zod-api": "8.4.0",
    "husky": "8.0.2",
    "jest": "29.3.1",
    "lint-staged": "13.0.3",
    "prettier": "2.8.0",
    "rimraf": "3.0.2",
    "semantic-release": "19.0.5",
    "source-map-support": "0.5.21",
    "ts-jest": "29.0.3",
    "ts-node": "10.9.1",
    "typescript": "4.9.3",
    "zod": "3.19.1"
  },
  "peerDependencies": {
    "express-zod-api": "*",
    "zod": "^3.19.0"
  },
  "peerDependenciesMeta": {
    "express-zod-api": {
      "optional": true
    },
    "zod": {
      "optional": true
    }
  },
  "lint-staged": {
    "*.{md,json,js,jsx,ts,tsx,yml,yaml}": [
      "yarn prettier --ignore-path .gitignore --write"
    ]
  },
  "config": {
    "commitizen": {
      "path": "cz-conventional-changelog"
    }
  },
  "dependencies": {
    "tslib": "2.4.1"
  },
  "packageManager": "yarn@3.3.0"
}
